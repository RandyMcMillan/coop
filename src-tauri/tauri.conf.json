{
  "productName": "Coop",
  "version": "0.0.0",
  "identifier": "com.tauri.dev",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "macOSPrivateApi": true,
    "withGlobalTauri": true,
    "security": {
      "csp": null,
      "assetProtocol": {
        "enable": true,
        "scope": [
          "$APPDATA/*",
          "$DATA/*",
          "$LOCALDATA/*",
          "$DESKTOP/*",
          "$DOCUMENT/*",
          "$DOWNLOAD/*",
          "$HOME/*",
          "$PICTURE/*",
          "$PUBLIC/*",
          "$VIDEO/*",
          "$APPCONFIG/*",
          "$RESOURCE/*"
        ]
      }
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
